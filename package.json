{
  "name": "moleculer-opentracing",
  "version": "1.0.0",
  "description": "Moleculer service for opentracing",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "moleculer-runner --repl test/__fixtures__/opentracing.service.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test index.js",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YourSoftRun/moleculer-opentracing.git"
  },
  "keywords": [
    "moleculer",
    "opentracing"
  ],
  "author": "Hugo Meyronneinc <hugo@yoursoft.run>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YourSoftRun/moleculer-opentracing/issues"
  },
  "homepage": "https://github.com/YourSoftRun/moleculer-opentracing#readme",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "dependencies": {
    "opentracing": "^0.14.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.0",
    "moleculer-repl": "^0.5.7",
    "eslint-plugin-jest": "^22.15.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "moleculer": "^0.13.9"
  }
}
